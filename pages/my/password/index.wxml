<!--pages/my/password/index.wxml-->
<view class="page">
    <view class="allset">
      <view class="passwork">
              <image class="iconset" src="../../../icons/passwork.png" />
              <input class="inputset" password placeholder="请输入新密码" bindinput="getpasswork"></input>
        </view>
        <view class="passwork">
          <image class="iconset" src="../../../icons/passwork.png" />
              <input class="inputset" password placeholder="请再次输入新密码" bindinput="getpasswork2"></input>
        </view>
    </view>
    <view class="save">
     <button class="comfirm" type="warn" hover-class="comfirm2" bindtap="comfirm" >保存</button>
   </view>
</view>
